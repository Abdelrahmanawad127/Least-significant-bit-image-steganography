image_path = input("enter the path of the image to decode: ")
with open(image_path, "rb") as f:
    data = f.read()
pixel_offset = data[10] + (data[11] << 8) + (data[12] << 16) + (data[13] << 24)
bits = ""
for i in range(pixel_offset, len(data)-2, 3):
    b = data[i] & 1
    g = data[i+1] & 1
    r = data[i+2] & 1
    bits += str(r) + str(g) + str(b)
